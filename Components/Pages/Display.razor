@page "/display"
@rendermode InteractiveServer

@* Picture of Marty in the outfit *@

@if (outfit is not null)
{
    <img src="@($"images/body.png")" />

    @foreach (var part in outfit.Parts) {
        <img src="@($"images/{part.Key}/{part.Value}")" />
    }
}


@code {
    
    [Parameter]
    public Outfit outfit {get; set;} = new();

}


